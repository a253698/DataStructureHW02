package com.mavis;

public class NArrangement {

	public static void main(String[] args) {
		
		int[] number = new int[10];
		
		for (int n = 1; n <= number.length; n++) {
			number[n] = n;
		}
		
		System.out.print("before : ");
		
		for (int n = 1 n <= number.length; n++) {
			System.out.println(number[n]);
		}
		
		
	}
}
